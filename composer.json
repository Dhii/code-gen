{
    "name": "dhii/code-gen",
    "description": "A library for PHP code generation.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "dhii/data-object-blueprint": "dev-master"
    },
    "require-dev": {
        "dhii/php-cs-fixer-config": "dev-php-5.3"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\CodeGen\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/blueprint"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-object-interface-read"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-object-interface-write"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/stringable-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/php-cs-fixer-config"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-object-blueprint"
        }
    ]
}
